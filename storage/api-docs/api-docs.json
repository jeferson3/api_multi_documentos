                "operationId": "ea8d3b7ddbc0c6fa15f201eb5f39c5cd",
                "responses": {
                    "200": {
                        "description": "Resposta com sucesso"
                    }
                },
                "security": [
                    {
                        "apiAuth": []
                    }
                ]
            }
        },
        "/auth/logout": {
            "post": {
                "tags": [
                    "Controllers - Autenticação"
