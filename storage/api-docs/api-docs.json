                    "description": "Salvar usuário",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "type": "String",
                                        "example": "admin"
                                    },
                                    "email": {
                                        "type": "String",
                                        "example": "admin@email.com"
                                    },
                                    "password": {
                                        "type": "String",
                                        "example": "password"
                                    },
                                    "company_id": {
                                        "type": "Int",
